commands:
    set_time_zone:
        command: ln -f -s /usr/share/zoneinfo/Asia/Seoul /etc/localtime

files:
    "/sbin/appstart" :
        mode: "000755"
        owner: webapp
        group: webapp
        content: |
            JAR_PATH=/var/app/current/application.jar

            killall java
            java -Dfile.encoding=UTF-8 -jar $JAR_PATH